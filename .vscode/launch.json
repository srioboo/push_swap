{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Push_swap Launch",
			"type": "cppdbg",
			"request": "launch",
			"program": "${fileDirname}/push_swap",
			"args": ["1 4 3 8 10 50"],
			"stopAtEntry": false,
			"cwd": "${fileDirname}",
			"environment": [],
			"externalConsole": false,
			"MIMode": "gdb",
			"setupCommands": [
				{
				"description": "Enable pretty-printing for gdb",
				"text": "-enable-pretty-printing",
				"ignoreFailures": true
				}
			],
			"preLaunchTask": "Build-push_swap",
			"miDebuggerArgs": "/bin/gdb"
		},
		{
			"name": "Push_swap Test-Launch",
			"type": "cppdbg",
			"request": "launch",
			"program": "${fileDirname}/push_swap",
			"args": ["1 4 3"],
			"stopAtEntry": false,
			"cwd": "${fileDirname}",
			"environment": [],
			"externalConsole": false,
			"MIMode": "gdb",
			"setupCommands": [
				{
				"description": "Enable pretty-printing for gdb",
				"text": "-enable-pretty-printing",
				"ignoreFailures": true
				}
			],
			"preLaunchTask": "Test-Build-push_swap",
			"miDebuggerArgs": "/bin/gdb"
		}
	]
}
