{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Build",
			"command": "/bin/gcc",
			"type": "cppbuild",
			"args": [
				"-fdiagnostics-color=always",
				"-g",
				"*.c",
				"-Iinclude",
				"-o",
				"${fileDirname}/push_swap"
			],
			"options": {
				"cwd": "${fileDirname}"
			},
			"problemMatcher": ["$gcc"],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"detail": "Task for build the program."
		},
		{
			"label": "Test-Build",
			"command": "/bin/gcc",
			"type": "cppbuild",
			"args": [
				"-fdiagnostics-color=always",
				"-g",
				"../push_swap_utils.c",
				"../ft_strlen.c",
				"../ft_isalpha.c",
				"../ft_atoi.c",
				"../ft_split.c",
				"../ft_substr.c",
				"../ft_strdup.c",
				"../push_swap_validate.c",
				"../operation_rev_rotate.c",
				"../operation_main.c",
				"../operation_push.c",
				"../operation_rotate.c",
				"../operation_swap.c",
				"../link_lstnew.c",
				"../link_lstlast.c",
				"../link_lstadd_back.c",
				"../link_lstsize.c",
				"main_test.c",
				"test_*.c",
				"-I../include",
				"-Iinclude",
				"-Llib",
				"-lfuntest",
				"-o",
				"${fileDirname}/push_swap"
			],
			"options": {
				"cwd": "${fileDirname}"
			},
			"problemMatcher": ["$gcc"],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"detail": "Task for build the program."
		}
	]
}
